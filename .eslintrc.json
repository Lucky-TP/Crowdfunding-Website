{
    "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
    "plugins": ["prettier, prettier-plugin-tailwindcss"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "tabWidth": 4,
                "printWidth": 100,
                "trailingComma": "es5",
                "semi": true
            }
        ]
    }
}
